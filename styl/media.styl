	.join_section .text
		font-size 17px
		margin-bottom 50px
	.team_section .slick-list
		padding 0
	
	footer .menu
		display none
	footer .bott_line .boxes >*
		width 100%
		margin 5px 0
		padding 0
	#coocies
		line-height: 1.1;
		.boxes
			padding 0 30px 0 10px
			max-width: 580px;
		img
			max-width: 50px;
		.viewmore_content
			display none
		.accept
