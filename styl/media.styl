@media (max-width 1700px)
	.content_big
		max-width 1280px
	#main_menu
		top 40px
		max-width none
		.links_block
			.link:not(:last-child)
				margin-right 35px
	.main_slider 
		box-sizing border-box
		padding-left 30px
		.slick-dots
			left -50px
		.round_wrp
			padding-top 80px
	.top_section
		padding-top 160px
		.hand
			max-width 530px
			bottom 0
		.top_section_bg
			min-width 1920px
			left 50%
			transform translateX(-50%)
	#side_dots
		left 10px
@media (max-width 1320px)
	.content
	.content_big
		max-width 800px
	.fixed_logo
		top 0
	.logo_wrp
		max-width: 106px;
	#main_menu_btn
		display block
		position fixed
		top 20px
		right 20px
		z-index 5
		width 30px
		transition filter 300ms
		&.fixed
			filter: contrast(0);
		img
			width 100%
	#main_menu
		position fixed
		top 0
		left 0
		right 0
		bottom 0
		background alpha(#0cb4ab,.9)
		z-index 6
		padding 30px
		margin 0
		display none
		&.fixed
			.links_block
				.link
					color #fff
			.socials
				filter: none
			.languages .link
				background #fff
				color #0d766f
		.close_menu
			display block
			position absolute
			top: 19px;
			right: 23px;
			img
				width 23px
		.mobile
			display block
			width 142px
			margin-bottom 30px
			filter: brightness(20);
		.links_block
			display flex
			flex-direction column
			align-items: flex-start;
			position relative
			left 0
			margin-left 0
			padding 0
			top 0
			.link
				text-align left
				font-size 24px
				font-weight 800
				text-transform uppercase
				&:not(:last-child)
					margin 0 0 20px
		.round_wrp
			padding-top 45px
			display flex
			max-width 300px
			.link
				margin 0 20px 0 0
				width 100%
		.socials
			width: 100%;
			display flex
		.languages
			flex-shrink 0
			margin-left: auto;
			padding 0
	.main_slider
		padding 0
		z-index 2
		.slick-dots
			display none!important
	.top_section
		min-height: auto;
		.hand
			transform scaleX(-1)
			max-width: 430px;
			left auto
			right 0
			bottom auto
			top 60px
			img:not(:first-child)
				position relative
			.text1
			.text2
				display none
	.top_section .top_section_bg
		min-width: auto
		width 1320px
		left: 50%;
		transform none
		margin-left -500px
	.why_invest
		.slick-list
			padding-left 0
			padding-bottom 20px
		.slick-track
			display flex
		&:after
			display none
		.slide
			height auto
			padding-bottom 10px
			margin 0 !important
			&:after
			&:before
				display none
				
			
	.about .boxes
		.video
			width 100%
			margin 0 0 30px
		.text
			width 100%
	footer .item_main:not(:last-child)
		margin-right: 50px;
	footer
		.link
			padding 0
			&:after
				display none
		.adreses img
			margin-right: 10px;
			max-width: 14px;
		.logo_block
			display none
		.coppy
			order 3
			width: 100%;
			padding-top 20px
		.bott_line
			text-align center
			
	footer .bott_line .madeby
		.mob
			display inline-block
		*
			margin 0 10px
			
@media (max-width 1200px)
	.btn
		height: 60px;
		font-size 16px
		min-width: 210px;
	#coocies
		font-size: 12px;
		img
			max-width: 60px;
	#side_dots
		display none
	.future_plans #future_plans_graphic
		margin 30px 0 40px
		
