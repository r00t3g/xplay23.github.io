.main_form
	position relative
	max-width 640px
	margin auto
	font-size 18px
	.input_wrp
		width 100%
		input
			box-sizing border-box
			text-align left
			width calc(100% - 110px)
			display block
			padding 27px 220px 28px 60px
			border-radius 100px
			border none
			background #f8f8f8
			box-shadow: inset 0px 2px 0px 0px rgba(241, 240, 240, 0.8);
			font-size 1em
	.input_submit
		width 220px
		height 100%
		right 0
		top 6px
		position absolute
		button
			font-size 1em
			cursor pointer
			border none
			background #0cb4ab
			color #fff
			width 100%
			height 100%
			border-radius 100px
			transition background 300ms
			border-bottom 2px solid #0cb4ab
			border-top 2px solid transparent
			&:hover
				background #00e0d4
