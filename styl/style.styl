			position absolute
			top 50%
			right 100%
			margin-right: -30px;
			max-width: 55%;
	
.form_popup
	pointer-events none
	position absolute
	left 0
	right 0
	top 50%
	opacity 0
	visibility hidden
	transform translateY(-50%) scale(0)
	background #fff
	padding 30px
	z-index 2
	border-radius: 45px;
	box-shadow: 9.762px 21.925px 24.96px 7.04px rgba(0, 0, 0, 0.17);
	display flex
	transition all 300ms
	.submited &
		pointer-events auto
		opacity 1
		visibility visible
		transform translateY(-50%) scale(1)
	svg
		width 80px
		margin-right 30px
	h4
		margin-bottom 5px
		font-weight bold
	.close
		position absolute
		top: 5px;
		right: 20px;
		font-size 30px
		transform rotate(45deg)
		cursor pointer

