global-reset()

@import "fonts.styl"


input
select
textarea
	font-family inherit
	font-size inherit
	color inherit
a
	font-family inherit
	text-decoration none
	color inherit
*
	-webkit-tap-highlight-color: transparent;


		
img
	max-width 100%
	height auto
	
input, select, textarea 
	font-size: 16px;

body
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
	color #333333
	font-size 16px
	line-height 1.5
	background #fff
	font-family: 'Lato';
*:focus
	outline none
strong
	font-weight: 800;
.sile_color
	color #0c9b93
.wrap_all
	width 100%
	position relative
	overflow hidden
.content
	max-width 1280px
	margin auto
	padding 0 20px
.content_big
	max-width 1680px
	margin auto
	padding 0 20px
	position relative
.big_head
	font-size 48px
	font-weight 800
	color #404543
	text-transform uppercase
	margin-bottom 48px
